"UnlitGeneric"
{
	"$additive" 1
	"$translucent" 1
	"$vertexcolor" 1
	"$vertexalpha" 1
	"$decal" 1

	"$alpha" 1	

	"$basetexture" "FX\Electro\Tazer\Tazer_Hit_Arcs_01_"


        "Proxies"
